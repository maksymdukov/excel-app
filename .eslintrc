{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "indent": "off",
    "max-len": "off"
  },
  "extends": ["google"]
}
